    "dev:app": "cross-env SKIP_PREFLIGHT_CHECK=true npm run start --workspace=app"
  },
  "eslintConfig": {
    "extends": "./node_modules/standard/eslintrc.json",
    "env": {
      "cypress/globals": true,
      "jest": true
    },
    "plugins": [
      "cypress"
    ]
  },
  "devDependencies": {
    "standard": "17.0.0",
    "eslint-plugin-cypress": "2.12.1"
  },
